<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android" >

    <LinearLayout
        android:id="@+id/otrabotaloLinearLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/status_linear_layout_bottom_margin"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginTop="@dimen/status_linear_layout_top_margin"
        android:background="@drawable/card"
        android:gravity="center"
        android:orientation="vertical" >

        <TextView
            android:id="@+id/otrabotaloTableRowLable"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:text="@string/otrabotalo_label" />

        <TableLayout
            android:id="@+id/otrabotaloTableLayout"
            android:layout_width="wrap_content"
            android:layout_height="0dip"
            android:layout_weight="1"
            android:gravity="center"
            android:stretchColumns="*" >

            <TableRow
                android:id="@+id/otrabotaloTableRowDays"
                android:layout_width="fill_parent"
                android:gravity="center_horizontal" >
                <TextView
                    android:id="@+id/otrabotaloTextViewDay1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:paddingBottom="@dimen/status_linear_layout_bottom_margin"
                    android:paddingTop="@dimen/status_linear_layout_top_margin"
                    android:text="1" />

                <TextView
                    android:id="@+id/otrabotaloTextViewDay2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:paddingBottom="@dimen/status_linear_layout_bottom_margin"
                    android:paddingTop="@dimen/status_linear_layout_top_margin"
                    android:text="2" />

                <TextView
                    android:id="@+id/otrabotaloTextViewDay3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:paddingBottom="@dimen/status_linear_layout_bottom_margin"
                    android:paddingTop="@dimen/status_linear_layout_top_margin"
                    android:text="3" />

                <TextView
                    android:id="@+id/otrabotaloTextViewDay4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:paddingBottom="@dimen/status_linear_layout_bottom_margin"
                    android:paddingTop="@dimen/status_linear_layout_top_margin"
                    android:text="4" />

                <TextView
                    android:id="@+id/otrabotaloTextViewDay5"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:paddingBottom="@dimen/status_linear_layout_bottom_margin"
                    android:paddingTop="@dimen/status_linear_layout_top_margin"
                    android:text="5" />
            </TableRow>

            <TableRow
                android:id="@+id/otrabotaloTableRowValues"
                android:layout_width="fill_parent"
                android:gravity="center_horizontal" >
                <TextView
                    android:id="@+id/otrabotaloTextViewValue1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:paddingBottom="@dimen/status_linear_layout_bottom_margin"
                    android:paddingTop="@dimen/status_linear_layout_top_margin"
                    android:text="56" />

                <TextView
                    android:id="@+id/otrabotaloTextViewValue2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:paddingBottom="@dimen/status_linear_layout_bottom_margin"
                    android:paddingTop="@dimen/status_linear_layout_top_margin"
                    android:text="23" />

                <TextView
                    android:id="@+id/otrabotaloTextViewValue3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:paddingBottom="@dimen/status_linear_layout_bottom_margin"
                    android:paddingTop="@dimen/status_linear_layout_top_margin"
                    android:text="67" />

                <TextView
                    android:id="@+id/otrabotaloTextViewValue4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:paddingBottom="@dimen/status_linear_layout_bottom_margin"
                    android:paddingTop="@dimen/status_linear_layout_top_margin"
                    android:text="84" />

                <TextView
                    android:id="@+id/otrabotaloTextViewValue5"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:paddingBottom="@dimen/status_linear_layout_bottom_margin"
                    android:paddingTop="@dimen/status_linear_layout_top_margin"
                    android:text="97" />
            </TableRow>
        </TableLayout>
    </LinearLayout>

</merge>